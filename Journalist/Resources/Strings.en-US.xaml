<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="#AppName#">Journalist</system:String>
    <system:String x:Key="#RunningBackground#">Running in the background</system:String>
    <system:String x:Key="#ShowWindow#">Show the window</system:String>
    <system:String x:Key="#Exit#">Exit</system:String>
    
</ResourceDictionary>